{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
   "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/ali-ben-hammad/github-readme-stats"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800"
        }
      ]
    }
  ]
}
